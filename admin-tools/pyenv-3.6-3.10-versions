# -*- shell-script -*-
# Sets PYVERSIONS to be pyenv versions that
# we can use in the master

if [[ $0 == ${BASH_SOURCE[0]} ]] ; then
    echo "This script should be *sourced* rather than run directly through bash"
    exit 1
fi

